// Clase que prueba la Sentencia While

class ClaseQueImplementaOtrasClases implements Sentencia, While{


    void metodoConWhilesAdentro(){
        while(condicion){
            pasanCosas();
            pasanCosas = 32;
            pasanCosas += hola;
        }

        while(3+3) cosas();

        while(repeat) a=b;

        while(condicion){
            while(condicion){
                while(condicion) tresWhiles();
            }
        }
    }
}